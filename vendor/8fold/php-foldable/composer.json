{
  "name": "8fold/php-foldable",
  "description": "Foundation for developing Fluent Interfaces and the Pipeline Pattern",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@8fold.pro"
    }
  ],
  "require": {
    "php": "~7"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.1"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\Foldable\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\Foldable\\Tests\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
